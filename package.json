    "copy-webpack-plugin": "^9.0.0",
    "cross-env": "7.0.3",
    "css-hot-loader": "1.4.4",
    "css-loader": "5.0.1",
    "html-webpack-plugin": "5.3.1",
    "image-webpack-loader": "^8.0.1",
